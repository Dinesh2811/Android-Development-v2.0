        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.AndroidDevelopmentV20"
        tools:targetApi="31">
        <activity
            android:name=".MainActivity"
            android:exported="true"
            android:label="@string/app_name"
            android:theme="@style/Theme.AndroidDevelopmentV20">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>

            <meta-data
                android:name="android.app.lib_name"
                android:value="" />
        </activity>

        <activity android:name=".java.fragment.Main"/>
        <activity android:name=".kotlin.fragment.Main"/>
        <activity android:name=".java.app.toolbar.MainActivity"/>
        <activity android:name=".java.app.toolbar.ToolbarMain"/>
        <activity android:name=".java.app.nav_drawer.NavigationDrawer"/>
        <activity android:name=".java.android_lifecycle.a.MainActivity"/>
        <activity android:name=".java.android_lifecycle.live_data.MainActivity"/>
        <activity android:name=".java.android_lifecycle.view_model.MainActivity"/>

        <activity android:name=".java.database.room.mvvm.MainActivity"/>
        <activity android:name=".kotlin.database.room.MainActivity"/>
        <activity android:name=".java.database.sql.MainActivity"/>
        <activity android:name=".java.app.nav_drawer.NavMain"/>
        <activity android:name=".java.app.tab_layout.TabLayoutMain"/>
        <activity android:name=".java.activity.GoTo_Class"/>
        <activity android:name=".java.activity.LayoutInflater_Class"/>
        <activity android:name=".java.api.volley.MainActivity"/>
        <activity android:name=".java.basic.BasicMainActivity"/>
        <activity android:name=".java.basic.image_view.test.MainActivity"/>
        <activity android:name=".java.basic.image_view.test.ReadExternalStorage"/>

        <activity android:name=".java.dialog.basic.Main"/>
        <activity android:name=".java.dialog.rv.Rv_as_Dialog"/>
        <activity android:name=".java.dialog.rv.recycler_view.basic.RvMain"/>
        <activity android:name=".java.dialog.rv.recycler_view.basic.NewLayout"/>
